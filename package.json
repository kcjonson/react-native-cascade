{
  "name": "babel-preset-react-native-cascade",
  "version": "1.0.3",
  "description": "A babel transform to apply matching css styles to React Native components",
  "main": "./src/index.js",
  "scripts": {
    "prepublishOnly": "npm run test",
    "unit-test": "jest",
    "lint": "eslint ./**/*.{js,jsx} --quiet",
    "test": "npm run lint && npm run unit-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kcjonson/react-native-cascade.git"
  },
  "author": "Kevin Jonson <kcjonson@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kcjonson/react-native-cascade/issues"
  },
  "homepage": "https://github.com/kcjonson/react-native-cascade#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.1.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-native": "^0.52.2",
    "react-test-renderer": "^16.2.0"
  },
  "dependencies": {
    "babylon": "^6.18.0",
    "resolve": "^1.5.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/enzyme-setup.js",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  }
}
